<keymap version="1" name="Windows copy" parent="$default">
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl alt minus" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl back_quote" />
    <keyboard-shortcut first-keystroke="shift ctrl #10000d6" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt end" />
  </action>
  <action id="QuickList.Custom">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
</keymap>